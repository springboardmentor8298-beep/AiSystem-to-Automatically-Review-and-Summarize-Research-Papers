  <script>
    $(document).on('click','.cite', function(){
      $id = $(this).attr('data-id');
      $('#mla').empty();
      $('#apa').empty();
      
      $.ajax({
        url:'https://www.ijisrt.com/search_mla',
        type:'post',
        data:{'id':$id},
        success:function(res){
          var obj= jQuery.parseJSON(res);
          if(obj){ 
            $('#mla').text(obj.mla);
            $('#apa').text(obj.apa);
            $('#mla_apa').modal('show');
          }
        }
      });
    });
</script>
